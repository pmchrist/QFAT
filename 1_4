   "source": [
    "yearly = []\n",
    "\n",
    "for i in range(len(df_results)):\n",
    "    totals = []\n",
    "    x = 0\n",
    "    for x in range(27):\n",
    "        if x % 3 == 0 and x != 0:\n",
    "            temp = df_results.values[i][x]\n",
    "            totals.append(float(temp))\n",
    "        x = x + 1\n",
    "    summed = sum(totals)\n",
    "    yearly.append(summed)\n",
    "    i = i + 1\n",
    "\n",
    "df_results['Totals'] = yearly\n",
    "\n",
    "df_results"
   ]
